<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.IdroidQuiz" version="1.0">
    <name>IDroidQuiz</name>
    <description>
        An Android and Iphone apps for both Polytech IRM staff and students,which one using it to publish some QCM and the other one to particpate to.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html"/>
    <access origin="*"/>
    <feature name="SQLitePlugin">
 		<param name="android-package" value="org.pgsqlite.SQLitePlugin" />
 	</feature>
 	<plugin name="SQLitePlugin" value="org.pgsqlite.SQLitePlugin"/>
</widget>
